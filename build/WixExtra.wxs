			Description="Collision detection and distance computation demo with basic scene"
			Id="CM_SP_bin.rlCollisionDemo.exe"
			Name="rlCollisionDemo"
			Target="[#CM_FP_bin.rlCollisionDemo.exe]"
			WorkingDirectory="CM_DP_bin"
		/>
<Shortcut
			Arguments="&quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlsg\unimation-puma560_boxes.xml&quot;"
			Description="Visualization demo with Unimation Puma 560"
			Id="CM_SP_bin.rlViewDemo.exe"
			Name="rlViewDemo"
			Target="[#CM_FP_bin.rlViewDemo.exe]"
			WorkingDirectory="CM_DP_bin"
		/>
<Shortcut
			Arguments="&quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlsg\unimation-puma560_boxes.xml&quot; &quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlkin\unimation-puma560.xml&quot;"
			Description="Visualization and kinematics demo with Unimation Puma 560"
			Id="CM_SP_bin.rlCoachKin.exe"
			Name="rlCoachKin"
			Target="[#CM_FP_bin.rlCoachKin.exe]"
			WorkingDirectory="CM_DP_bin"
		/>
<Shortcut
			Arguments="&quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlsg\unimation-puma560_boxes.xml&quot; &quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlmdl\unimation-puma560.xml&quot;"
			Description="Visualization and kinematics demo with Unimation Puma 560"
			Id="CM_SP_bin.rlCoachMdl.exe"
			Name="rlCoachMdl"
			Target="[#CM_FP_bin.rlCoachMdl.exe]"
			WorkingDirectory="CM_DP_bin"
		/>
<Shortcut
			Arguments="&quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlsg\unimation-puma560_boxes.xml&quot; &quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlmdl\unimation-puma560.xml&quot;"
			Description="Simulation demo with Unimation Puma 560"
			Id="CM_SP_bin.rlSimulator.exe"
			Name="rlSimulator"
			Target="[#CM_FP_bin.rlSimulator.exe]"
