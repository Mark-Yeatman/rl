			Arguments="&quot;[INSTALL_ROOT]share\rl-0.7.0\examples\rlplan\unimation-puma560_boxes_rrtConCon.mdl.xml&quot;"
			Description="Path planning demo with Unimation Puma 560 and RRT algorithm"
			Id="CM_SP_bin.rlPlanDemo.exe"
			Name="rlPlanDemo"
			Target="[#CM_FP_bin.rlPlanDemo.exe]"
			WorkingDirectory="CM_DP_bin"
		/>
<Shortcut
			Description="Viewer for VRML and Open Inventor files"
			Id="CM_SP_bin.wrlview.exe"
			Name="wrlview"
			Target="[#CM_FP_bin.wrlview.exe]"
			WorkingDirectory="CM_DP_bin"
		/>
					</Component>
				</Directory>
			</Directory>
		</DirectoryRef>
